      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <tabs>
          <Tab text="Static Data Visualization">
            <content>
                <fx:include fx:id="staticDataPane" source="WormViewerFXStatic.fxml"/>
            </content>
          </Tab>
          <Tab text="Time Based Data Visualization">
            <content>
              <fx:include fx:id="dynamicDataPane" source="WormViewerFXDynamic.fxml"/>
            </content>
          </Tab>
            <Tab text="Data Management">
              <content>
                <fx:include fx:id="dataManagementPane" source="WormViewerFXData.fxml"/>
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
